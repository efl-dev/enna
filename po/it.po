#, c-format
msgid "Enna MediaCenter\n"
msgstr "Enna Media Center\n"

#: src/bin/enna.c:498
#, c-format
msgid " Usage: %s [options ...]\n"
msgstr "Uso: %s [opzioni...]\n"

#: src/bin/enna.c:499
#, c-format
msgid " Available options:\n"
msgstr " Opzioni disponibili:\n"

#: src/bin/enna.c:500
#, c-format
msgid "  -c, (--config):  Specify configuration file to be used.\n"
msgstr "  -c, (--config):   specifica il file di configurazione da usare.\n"

#: src/bin/enna.c:501
#, c-format
msgid "  -f, (--fs):      Force fullscreen mode.\n"
msgstr "  -f, (--fs):       forza modalità a pieno schermo.\n"

#: src/bin/enna.c:502
#, c-format
msgid "  -h, (--help):    Display this help.\n"
msgstr "  -h, (--help):     mostra questo messaggio d'aiuto.\n"

#: src/bin/enna.c:503
#, c-format
msgid "  -t, (--theme):   Specify theme name to be used.\n"
msgstr "  -t, (--theme):    specifica il nome del tema da usare.\n"

#: src/bin/enna.c:504
#, c-format
msgid "  -g, (--geometry):Specify window geometry. (geometry=1280x720)\n"
msgstr ""
"  -g, (--geometry): specifica la geometria della finestra. "
"(geometry=1280x720)\n"

#: src/bin/enna.c:505
#, c-format
msgid ""
"  -g, (--geometry):Specify window geometry and offset. "
"(geometry=1280x720:10:20)\n"
msgstr ""
"  -g, (--geometry): specifica la geometria della finestra. "
"(geometry=1280x720:10:20)\n"

#: src/bin/enna.c:506
#, c-format
msgid "  -p, (--profile): Specify display profile\n"
msgstr "  -p, (--profile): Specifica profilo schermo\n"

#: src/bin/enna.c:507
#, c-format
msgid "    Supported: "
msgstr "    Supportato: "

#: src/bin/enna.c:511
#, c-format
msgid "  -V, (--version): Display Enna version number.\n"
msgstr "  -V, (--version):  mostra il numero di versione di Enna.\n"

